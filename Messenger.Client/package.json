{
  "name": "messupp",
  "version": "1.0.0",
  "description": "Simple messenger.",
  "main": "main.js",
  "icon": "./icon.ico",
  "scripts": {
    "start": "electron . --in",
    "buildAll": "electron-packager . MessUpp --overwrite --platform=win32 --arch=all --version=5.0.0",
    "buildWin32": "electron-packager . MessUpp --overwrite --platform=win32 --arch=ia32 --version=5.0.0",
    "buildWin64": "electron-packager . MessUpp --overwrite --platform=win32 --arch=x64 --version=5.0.0",
    "pack": "electron-builder --dir ",
    "dist": "electron-builder --w --ia32"
  },
  "author": "Barney",
  "license": "ISC",
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-packager": "5.0.0",
    "electron-prebuilt": "^1.4.13"
  },
  "build": {
    "appId": "com.messup.app"
  },
  "dependencies": {
    "jquery": "3.2.1",
    "rest": "^2.0.0"
  }
}
